   "source": [
    "def perfectnum(n):\n",
    "    sum=0\n",
    "    for i in range(1,n//2+1):\n",
    "        if(n%i==0):\n",
    "            sum=sum+i\n",
    "    if(sum==n):\n",
    "        print(n,\"is perfect number\")\n",
    "number=6        \n",
    "perfectnum(number)      "
   ]

    "def fabonacci_series(t):\n",
    "    f=0\n",
    "    s=1\n",
    "\n",
    "    count=0\n",
    "    while(count<t):\n",
    "        print(f)\n",
    "        temp=f+s\n",
    "        f=s\n",
    "        s=temp\n",
    "        count+=1\n",
    "nt=5 #no of terms        \n",
    "fabonacci_series(nt)    \n",
    "    "
   ]

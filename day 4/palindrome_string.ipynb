   "source": [
    "\n",
    "def palindrome_or_not(a):\n",
    "    n=len(a)\n",
    "    f=0\n",
    "    for i in range(n//2):\n",
    "        if(a[i]==a[n-i-1]):\n",
    "            f=1\n",
    "    if(f==1):\n",
    "        print(\"palindrome\")\n",
    "st=\"madam\"        \n",
    "palindrome_or_not(st)        "
   ]
